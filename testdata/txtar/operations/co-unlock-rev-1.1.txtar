-- generator.sh --
#!/usr/bin/env bash
set -euo pipefail
export TZ=UTC LOGNAME=tester USER=tester
unset RCSINIT

OUT="73-co-unlock-rev-1.1.txtar"
tmp="$(mktemp -d)"
trap 'rm -rf "$tmp"' EXIT
cd "$tmp"

printf "ONE\nTWO\n" > file.txt

# Initial check-in (1.1)
ci -q -i -u -m"r1" -wtester -d'2020-01-01 00:00:00Z' file.txt </dev/null
# Disable strict locking
rcs -q -U file.txt
# Check out with lock on 1.1
co -q -l1.1 file.txt
# Ensure file is back in RCS control but locked?
# co -l checks out.
# Now we have file.txt (writable) and RCS file has lock.

cp file.txt,v input.txt,v
rm file.txt

# Run co with -u1.1 (unlock)
co -q -u1.1 file.txt

cat > "$OLDPWD/$OUT" <<EOF
# -- description.txt --
co checkout revision 1.1 and unlock

# -- options.conf --
{"args": ["-q","-u1.1","input.txt"] }

# -- input.txt,v --
$(cat input.txt,v)

# -- tests.txt --
co

# -- expected.txt --
ONE
TWO

# -- expected.txt,v --
$(cat file.txt,v)
EOF
